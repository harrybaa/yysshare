/**index.wxss**/
.container {
    background: #f2e7cb;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
}

.coverImage {
    width: 100%;
}

.headWrapper {
    width: 100%;
}

/**users**/
.userinfo, {
    position: fixed;
    width: 100rpx;
    top: 30rpx;
    right: 20rpx;
    text-align: center;
}

.userinfo-avatar {
    width: 100rpx;
    height: 100rpx;
    border-radius: 50%;
    border: 5rpx solid #c3c3c3;
}

.userinfo-nickname {
    font-size: 19rpx;
    color: #223022;
    text-align: center;
}

/**picker**/
.picker-section {
    width: 100%;
    padding: 0 20rpx;
    text-align: center;
    margin: 10rpx 20rpx;
}

.server-picker {
    width: 400rpx;
    text-align: center;
    display: flex;
}

.server-picker-name, .server-picker-item {
    margin: 0 10rpx;
}

.server-picker-name {
    padding: 5rpx 0;
}

.server-picker-item {
    background-color: #eaeaea;
    width: 140rpx;
    padding: 5rpx 20rpx;
    border-radius: 15rpx;
    text-align: left;
    box-shadow: #666 1px 3px 10px
}

/**list item**/
.list-item {
    width: 100%;
    height: 100rpx;
    margin-top: 60rpx;
}

.list-item-wrapper {
    width: 628rpx;
    height: 120rpx;
    border-radius: 20rpx;
    display: flex;
    background-color: #e8f3eb;
    background-image: url('data:image/png;base64,');
    background-size: cover;
    box-shadow: #b4ccc4 2rpx 6rpx 20rpx;
}

.zaier-profile {
    width: 150rpx;
    height: 150rpx;
    margin-left: 20rpx;
    margin-right: 10rpx;
    margin-top: -28rpx;
    flex-direction: row;
}

.zaier-profile-image {
    width: 100%;
}

.list-item-info {
    flex-direction: row;
    padding: 10rpx;
}

.list-item-service {
    display: flex;
    margin: 5rpx 0rpx;
}

.list-item-s1 {
    flex-direction: row;
    height: 34rpx;
    background-color: #332615;
    padding: 5rpx 15rpx;
    border-radius: 10rpx;
    color: #b4ccc4;
    font-size: 24rpx;
    font-weight: bold;
    font-family: 'KaiTi', 'DFKai-SB', 'SimSun';
}

.list-item-s2 {
    flex-direction: row;
    height: 34rpx;
    padding: 5rpx 15rpx;
    color: #332615;
    font-size: 24rpx;
    font-weight: bold;
    font-family: 'KaiTi';
}

.list-item-title {
    font-size: 36rpx;
}

.view-times-wrapper {
    flex-direction: row;
    width: 34rpx;
    padding-top: 4rpx;
}

.view-times-wrapper text {
    font-size: 24rpx;
    color: #666;
}

/**on post button**/
.onpost-wrapper {
    height: 100rpx;
    margin-top: 60rpx;
    margin-bottom: 40rpx;
    text-align: right;
}

.onpost-wrapper-button {
    padding-top: 10rpx;
    width: 400rpx;
    height: 82rpx;
    border-radius: 50rpx;
    background-color: #912e2a;
    color: #eeeeee;
    font-size: 52rpx;
    box-shadow: #332615 2rpx 6rpx 20rpx;
    text-align: center;
}

/** footer **/
.footer-info {
    margin-top: -150rpx;
    height: 0;
    text-align: center;
}

.footer-info-version {
    padding: 1rpx 15rpx;
    height: 0;
    width: 120rpx;
    height: 40rpx;
    background: #b4ccc4;
    box-shadow: #b4ccc4 2rpx 6rpx 20rpx;
    border-radius: 10rpx;
    font-size: 24rpx;
    color: #666;
}

.footer-info-note {
    margin-top: 100rpx;
    padding: 0 80rpx;
    font-size: 24rpx;
    color: #e8f3eb;
}